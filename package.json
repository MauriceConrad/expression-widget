{
  "name": "expression-widget",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "files": [
    "package.json",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/expression-input.es.js",
      "require": "./lib/expression-input.umd.js"
    },
    "./style.css": "./lib/style.css"
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "npm link && npm-watch build",
    "preview": "vite preview"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,vue,html,scss",
      "quiet": false
    }
  },
  "dependencies": {
    "@bluepic/core": "^0.0.139-beta.1",
    "@vicons/ionicons5": "^0.12.0",
    "codeflask": "^1.4.1",
    "color": "^4.2.3",
    "css-expression": "^0.0.8",
    "esprima": "^4.0.1",
    "eventemitter3": "^5.0.0",
    "is-var-name": "^2.0.0",
    "js-beautify": "^1.14.7",
    "lodash": "^4.17.21",
    "naive-ui": "^2.34.2",
    "sortablejs": "^1.15.0",
    "uniqid": "^5.4.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/codeflask": "^1.4.3",
    "@types/color": "^3.0.3",
    "@types/esprima": "^4.0.3",
    "@types/js-beautify": "^1.13.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.10",
    "@types/sortablejs": "^1.15.0",
    "@types/uniqid": "^5.3.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "npm-watch": "^0.11.0",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.6",
    "vue-tsc": "^0.40.4"
  }
}
